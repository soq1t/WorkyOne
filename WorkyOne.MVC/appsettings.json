{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtOptions": {
    "CookiesName": "JwtToken",
    "ExpirationMinutes": 30
  },
  "SessionOptions": {
    "CookiesName": "SessionToken",
    "ExpirationDays": 20
  },
  "LoginRedirectionPath": "/account/signin",
  "PaginationOptions": {
    "PageSize":  20
  }
}
